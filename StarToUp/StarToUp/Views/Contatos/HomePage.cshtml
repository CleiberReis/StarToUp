@model StarToUp.Models.Contato

@{
    ViewBag.Title = "Create";
}

<!-- About -->
<div id="about" class="section md-padding">

    <!-- Container -->
    <div class="container">
        <br /><br />
        <!-- Row -->
        <div class="row">
            <!-- Section header -->
            <div class="section-header text-center">
                <h2 class="title">Seja Bem-vindo!</h2>
            </div>
            <!-- /Section header -->
            <!-- about -->
            <div class="col-md-4">
                <div class="about">

                    <img style="width:70px; height:50px;" src="~/Content/img/startup.png" />
                    <h3>A StartToUp</h3>
                    <p>
                        Uma plataforma criada para divulgar diversos segmentos de startups, facilitando
                        que as empresas encontrem as ideias e soluções que precisam para levar tecnologia e inovação
                        para dentro de suas companhias.
                    </p>

                </div>
            </div>
            <!-- /about -->
            <!-- about -->
            <div class="col-md-4">
                <div class="about">

                    <img style="width:70px; height:50px;" src="~/Content/img/search.png" />
                    <h3>Facilidade</h3>
                    <p>
                        Facilitaremos o encontro entre empresas e as melhores Startups, filtrando aquela que agregará valor ao negócio e reduzindo o tempo de pesquisa.
                        As startups serão elencadas por segmentação.
                    </p>

                </div>
            </div>
            <!-- /about -->
            <!-- about -->
            <div class="col-md-4">
                <div class="about">

                    <img style="width:60px; height:50px;" src="~/Content/img/calendar (1).png" />
                    <h3>Fique por dentro!</h3>
                    <p>Saiba de todos os eventos relacionados à Startups que estarão prestes a acontecer, seu conteúdo e local.</p>
                    <br />
                    <br />
                    <br />
                </div>
            </div>
            <!-- /about -->

        </div>
        <!-- /Row -->

    </div>
    <!-- /Container -->

</div>
<!-- /About -->
<!-- Portfolio -->
<div id="portfolio" class="section md-padding bg-grey">

    <!-- Container -->
    <div class="container">

        <!-- Row -->
        <div class="row">
            <br /><br />
            <!-- Section header -->
            <div class="section-header text-center">
                <h2 class="title">Startups em Destaque</h2>
            </div>
            <!-- /Section header -->
            <!-- Work -->
            <!-- /Work -->
        </div>
        <!-- /Container -->

    </div>

</div>
<!-- /Portfolio -->
<!-- Numbers -->
<div id="numbers" class="section sm-padding">

    <!-- Background Image -->
    <div class="bg-img" style="background-image: url('../../Content/img/background2.jpg');">
        <div class="overlay"></div>
    </div>
    <!-- /Background Image -->
    <!-- Container -->
    <div class="container">
        <br /><br />
        <!-- Row -->
        <div class="row">

            <!-- number -->
            <div class="col-sm-3 col-xs-6">
                <div class="number">
                    <i class="fa fa-users"></i>
                    <h3 class="white-text"><span class="counter">451</span></h3>
                    <span class="white-text">Startups</span>
                </div>
            </div>
            <!-- /number -->
            <!-- number -->
            <div class="col-sm-3 col-xs-6">
                <div class="number">
                    <i class="fa fa-trophy"></i>
                    <h3 class="white-text"><span class="counter">12</span></h3>
                    <span class="white-text">Awards won</span>
                </div>
            </div>
            <!-- /number -->
            <!-- number -->
            <div class="col-sm-3 col-xs-6">
                <div class="number">
                    <i class="fa fa-coffee"></i>
                    <h3 class="white-text"><span class="counter">154</span>K</h3>
                    <span class="white-text">Cups of Coffee</span>
                </div>
            </div>
            <!-- /number -->
            <!-- number -->
            <div class="col-sm-3 col-xs-6">
                <div class="number">
                    <i class="fa fa-file"></i>
                    <h3 class="white-text"><span class="counter">45</span></h3>
                    <span class="white-text">Projetos de sucesso</span>
                </div>
            </div>
            <!-- /number -->

        </div>
        <!-- /Row -->

    </div>
    <!-- /Container -->

</div>
<!-- /Numbers -->
<!-- Blog -->
<div id="blog" class="section md-padding bg-grey">

    <!-- Container -->
    <div class="container">
        <br /><br />
        <!-- Row -->
        <div class="row">

            <!-- Section header -->
            <div class="section-header text-center">
                <h2 class="title">Eventos</h2>
            </div>
        </div>
        <!-- /Row -->

    </div>
    <!-- /Container -->

</div>
<!-- /Blog -->
<!-- Contact -->
<div id="contact" class="section md-padding">

    <!-- Container -->
    <div class="container">
        <br /><br />
        <!-- Row -->
        <div class="row">

            <!-- Section-header -->
            <div class="section-header text-center">
                <h2 class="title">Entre em contato conosco</h2>
            </div>
            <!-- /Section-header -->
            <!-- contact -->
            <div class="col-sm-4">
                <div class="contact">
                    <i class="fa fa-phone"></i>
                    <h3>Telefone</h3>
                    <p>(12) 98505-7686</p>
                </div>
            </div>
            <!-- /contact -->
            <!-- contact -->
            <div class="col-sm-4">
                <div class="contact">
                    <i class="fa fa-envelope"></i>
                    <h3>Email</h3>
                    <p>startoupcontact@gmail.com</p>
                </div>
            </div>
            <!-- /contact -->
            <!-- contact -->
            <div class="col-sm-4">
                <div class="contact">
                    <i class="fa fa-map-marker"></i>
                    <h3>Endereço</h3>
                    <p>Av. Castelo Branco, nº432, São Paulo/SP</p>
                </div>
            </div>
            <!-- /contact -->
            <!-- contact form -->
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-horizontal">
                    <h4>Contato</h4>
                    <hr />
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-6">
                                @Html.EditorFor(model => model.Nome, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.Nome, "", new { @class = "text-danger" })
                                <label>Nome</label>
                            </div>
                            <div class="col-md-6">
                                @Html.EditorFor(model => model.Email, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.Email, "", new { @class = "text-danger" })
                                <label>E-mail</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.Assunto, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.Assunto, "", new { @class = "text-danger" })
                        <label>Assunto</label>
                    </div>
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.Mensagem, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.Mensagem, "", new { @class = "text-danger" })
                        <label>Mensagem</label>
                    </div>
                </div>
                    <div class="col-md-offset-5 col-md-10">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
            }
        </div>
        <!-- /Row -->

    </div>
    <!-- /Container -->
</div>
<!-- /Contact -->
