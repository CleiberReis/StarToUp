    <!-- Container -->
    <div class="container-fluid">
        <br /><br />
        <!-- Row -->
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <!-- footer logo -->
                <div class="footer-logo">
                    <a href="~/Views/Home/Index.cshtml"><img src="~/Content/img/Logo_StarToUp1.png" alt="logo"></a>
                </div>
                <!-- /footer logo -->
                <!-- footer follow -->
                <ul class="footer-follow">
