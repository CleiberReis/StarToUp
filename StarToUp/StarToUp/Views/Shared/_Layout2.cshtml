<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>@ViewBag.Title - StarToUp</title>

    <script type="text/javascript">
        function redirecionar() {
            if (confirm("Deseja sair da sessão?")) {
                window.location = "http://localhost:50072/Logon/Logar";
            }
            else {
                return false;
            }
        }
    </script>

    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700%7CVarela+Round" rel="stylesheet">
    @Styles.Render("~/Content/css")


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <!-- Header -->
    <header id="home">
        <!-- Background Image -->
        <div class="bg-img" style="background-image: url('../../Content/img/backgroun6.jpg');">
            <div class="overlay"></div>
        </div>
        <!-- /Background Image -->
        <!-- Nav -->
        <nav id="nav" class="navbar nav-transparent">
            <div class="container">

                <div class="navbar-header">
                    <!-- Collapse nav button -->
                    <div class="nav-collapse">
                        <span></span>
                    </div>
                    <!-- /Collapse nav button -->
                </div>

                <!--  Main navigation  -->
                <ul class="main-nav nav navbar-nav navbar-right">
                    <li><a href="#home">Home</a></li>
                    <li><a href="#about">Sobre o site</a></li>
                    <li><a href="#portfolio">Startups em Destaque</a></li>
                    <li><a href="#contact">Contato</a></li>
                    <li class="has-dropdown">
                        <a href="#blog">Sign In</a>
                        <ul class="dropdown">
                            <li>@Html.ActionLink("Sign In", "Logar", "Logon")</li>
                        </ul>
                    </li>
                    <li><a href="#" onclick="redirecionar();">Sair</a></li>
                    @*<li>@Html.ActionLink("Sair", "Logoff", "Logon")</li>*@
                </ul>
                <!-- /Main navigation -->
            </div>
        </nav>
        <!-- /Nav -->
        <!-- home wrapper -->
        <div class="home-wrapper">
            <div class="container">
                <div class="row">

                    <!-- home content -->
                    <div class="col-md-10 col-md-offset-1">
                        <div class="home-content">
                            <h1 class="white-text">StarToUp</h1>
                            <p class="white-text">
                                Venha decolar com a gente!
                            </p>
                            <button class="btn-lg btn-outline-primary">@Html.ActionLink("Cadastrar Empresa", "Create", "EmpresaCadastros")</button>
                            <button class="btn-lg btn-outline-primary">@Html.ActionLink("Cadastrar Startup", "Create", "StartupCadastros")</button>
                        </div>
                    </div>
                    <!-- /home content -->

                </div>
            </div>
        </div>
        <!-- /home wrapper -->

    </header>
    @RenderBody()
